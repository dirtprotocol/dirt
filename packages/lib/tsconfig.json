{
  "include": [
      "src/**/*.ts",
  ],
  "compilerOptions": {
      "allowJs": false,
      "allowSyntheticDefaultImports": true,
      "checkJs": false,
      "declaration": true,
      "downlevelIteration": true,
      "importHelpers": true,
      "jsx": "react",
      "lib": [
          "dom",
          "es2015",
          "es2016",
          "es2017",
          "esnext"
      ],
      "module": "commonjs",
      "moduleResolution": "node",
      "noImplicitReturns": true,
      "noImplicitThis": true,
      "outDir": "./dist",
      "skipLibCheck": true,
      "sourceMap": true,
      "strictNullChecks": false,
      "suppressImplicitAnyIndexErrors": true,
      "target": "es5",
      "stripInternal": true
  },
  "typedocOptions": {
    "mode": "file",
    "out": "docs",
    "exclude": "test",
    "theme": "minimal",
    "ignoreCompilerErrors": true,
    "excludePrivate": true,
    "excludeProtected": true,
    "excludeNotExported": true,
    "target": "ES5",
    "moduleResolution": "node",
    "preserveConstEnums": true,
    "stripInternal": true,
    "suppressExcessPropertyErrors": true,
    "suppressImplicitAnyIndexErrors": true,
    "module": "commonjs"
  }
}
